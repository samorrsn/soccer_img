<% provide(:title, "Edit team member") %>
<h1>Edit team member information</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@user) do |f| %>
     <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :member_first_name %>
      <%= f.text_field :member_first_name %>

      <%= f.label :member_last_name %>
      <%= f.text_field :member_last_name %>

      <%= f.label :type %>
      <%= f.text_field :type %>

      <%= f.label :user_id %>
      <%= f.text_field :user_id %>

      <%= f.label :team_id %>
      <%= f.text_field :team_id %>

      <%= f.label :updated_at %>
      <%= f.text_field :updated_at %>

      <%= f.label :created_at %>
      <%= f.text_field :created_at %>

      <%= link_to 'Edit player positions', teams_profile_path %> 

      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>

    
  </div>
</div>

<%= link_to 'Show', @team_member %> |
<%= link_to 'Back', team_members_path %>
